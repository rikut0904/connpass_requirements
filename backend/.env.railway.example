# 必須
DATABASE_URL=<Railway PostgreSQLの接続文字列>
CORS_ALLOW_ORIGINS=<フロントエンドのURL>
JWT_SECRET=<32文字以上のランダム文字列>
# Discord API
DISCORD_CLIENT_ID=
DISCORD_CLIENT_SECRET=
DISCORD_REDIRECT_URI=
DISCORD_BOT_TOKEN=
